<head>
  <link rel="stylesheet" href="/css/containerElements.css">
</head>

<section class="containerElements__sectionArticles">
  <article class="sectionArticles__divContent-containerElements">
    <div class="divContent__divT-containerElements">
      <span>
        <h3><%- oneTitle %></h3>
      </span>
      <% if (validation_secondTitle) { %>
      <span>
        <h3><%- twoTitle %></h3>
      </span>
      <% } %>
    </div>

    <div class="divContent__divP-containerElements">
      <p>
        <%- paragraphInformation %>
      </p>

      <% if (validation_contentList) { %>
      <ul>
        <% contentList.forEach(({ l }) => { %>
        <li>
          <span>
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#63b9b0" class="bi bi-check-circle-fill" viewBox="0 0 16 16">
              <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0m-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z" />
            </svg>
          </span>
          <p>
            <%- l %>
          </p>
        </li>
        <% }) %>
      </ul>
      <% } %>
    </div>

    <% if (validation__containButtom) { %>
    <div class="divContent__divB-containerElements">
      <button class="navbar__divButton" data-href="/<%= redirectPage %>" data-type="contactar">
        <%- textButton %>
      </button>
    </div>
    <% } %>
  </article>
</section>

<script src="/js/redirectPage.js" defer></script>